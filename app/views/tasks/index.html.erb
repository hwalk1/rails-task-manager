<h1>My tasks:</h1>
<ul>
  <% @tasks.each do |task| %>
    <li><%= task.completed %>
    <%= link_to task.title, task_path(task)%>
    <%= link_to "Edit", task_edit_path(task) %>
    <%= link_to "Delete",
                task_path(task),
                method: :delete,
                data: { confirm: "Are you sure?"} %>
    </li>
  <% end %>
</ul>
<%= link_to 'Create new task', task_new_path%>
